{
  "name": "@apps/account",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:css": "postcss build public/stylesheets/tailwind.css -o public/stylesheets/style.css",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:online": "cross-env IS_DDB_LOCAL= nest start",
    "deploy": "sls deploy --verbose",
    "package": "sls package",
    "lint": "eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "test:cov": "jest --verbose --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ddb:install": "sls dynamodb install",
    "ddb:start": "sls dynamodb start",
    "sls:start": "cross-env IS_NOT_SLS= sls offline start",
    "sls:online": "cross-env IS_NOT_SLS= cross-env IS_DDB_LOCAL= sls offline",
    "genres": "ts-node genres.ts -- \"src/**/*.schema.ts\" \"resources/dynamodb.yml\""
  },
  "dependencies": {
    "@nestjs/common": "^7.3.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.3.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/graphql": "^7.5.5",
    "apollo-server-express": "^2.16.0",
    "aws-serverless-express": "^3.3.8",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "connect-dynamodb": "2.0.4",
    "dynamoose": "^2.2.1",
    "graphql": "^14.0.0",
    "lodash": "4.17.19",
    "nestjs-dynamoose": "0.2.5",
    "otplib": "12.0.1",
    "qrcode": "1.4.4",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "hbs": "~4.1.1",
    "passport-strategy": "~1.0.0",
    "nestjs-session": "~1.0.1",
    "express-session": "~1.17.1",
    "connect-flash": "~0.1.1",
    "uuid": "8.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@nestjs/cli": "7.4.1",
    "@nestjs/schematics": "7.0.1",
    "@nestjs/testing": "7.3.2",
    "@shelf/jest-dynamodb": "1.7.0",
    "@fullhuman/postcss-purgecss": "2.3.0",
    "@types/aws-lambda": "8.10.59",
    "@types/aws-serverless-express": "3.3.3",
    "@types/body-parser": "1.17.0",
    "@types/connect-flash": "0.0.34",
    "@types/express": "4.17.7",
    "@types/express-session": "1.15.8",
    "@types/jest": "26.0.7",
    "@types/js-yaml": "3.12.5",
    "@types/lodash": "4.14.108",
    "@types/node": "14.0.26",
    "@types/passport": "0.4.5",
    "@types/serverless": "1.72.5",
    "@types/supertest": "2.0.10",
    "@types/uuid": "8.0.0",
    "@types/qrcode": "1.3.5",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.7.0",
    "pascal-case": "3.1.1",
    "cross-env": "7.0.2",
    "cssnano": "4.1.10",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "glob-promise": "3.4.0",
    "jest": "26.1.0",
    "js-yaml": "3.14.0",
    "lint-staged": "10.2.11",
    "postcss-cli": "7.1.1",
    "prettier": "2.0.5",
    "serverless": "1.76.1",
    "serverless-dynamodb-local": "0.2.39",
    "serverless-offline": "6.5.0",
    "serverless-plugin-typescript": "1.1.9",
    "supertest": "4.0.2",
    "ts-jest": "26.1.3",
    "ts-loader": "8.0.1",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.7",
    "tailwindcss": "^1.5.2"
  },
  "jest": {
    "preset": "@shelf/jest-dynamodb",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 45000
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
